{
	"name": "Hewalex2MQTT-hvdb",
	"version": "0.2.0",
	"slug": "hewalex",
	"description": "Hewalex heat pump monitoring",
	"url": "https://github.com/hvdb/hewalex2mqtt-homeassistant-add-on/",
	"arch": [
		"armv7",
		"aarch64",
		"amd64",
		"i386"
	],
	"startup": "application",
	"init": "false",
	"boot": "auto",
	"host_network": "true",
	"options": {
